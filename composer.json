    "require": {
        "php": "~7.0",
        "chubbyphp/chubbyphp-error-handler": "~1.0",
        "chubbyphp/chubbyphp-session": "~1.0",
        "psr/log": "~1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~5.5",
        "pimple/pimple": "~3.0"
    },
    "suggest": {
        "pimple/pimple": "Is needed for the CsrfProvider"
    },
    "autoload": {
        "psr-4": { "Chubbyphp\\Csrf\\": "src/" }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
